{% set template = response.meta.template %}
{% set seo = response.meta.seo %}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="canonical" href="{{ seo.canonical ?? request.url }}">
    <meta name="description" content="{{ seo.metaDescription }}">
    <title>{{ seo.title }}</title>
    <link rel="icon" href="//{{ domain }}/file/images/favicon.ico" type="image/x-icon">

    <!-- Bootstrap -->
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" rel="stylesheet">
    <link href="//{{ domain }}/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet">
    <link href="//{{ domain }}{{ css('/css/style.css') }}" rel="stylesheet">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    <script type="text/javascript" src="//{{ domain }}{{ js('/js/script.js') }}"></script>

    <!--   <link rel="stylesheet" href="/style3.css" type="text/css" media="screen" />
        <link rel="stylesheet" href="/common.css" type="text/css" media="screen" />-->

    <!-- google analytics -->
    {% if response.meta.googleTrackingCode %}
        <script>
            (function (i, s, o, g, r, a, m) {
                i['GoogleAnalyticsObject'] = r;
                i[r] = i[r] || function () {
                            (i[r].q = i[r].q || []).push(arguments)
                        }, i[r].l = 1 * new Date();
                a = s.createElement(o),
                        m = s.getElementsByTagName(o)[0];
                a.async = 1;
                a.src = g;
                m.parentNode.insertBefore(a, m)
            })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

            ga('create', '{{ response.meta.googleTrackingCode }}', 'auto');
            ga('send', 'pageview');

        </script>
    {% endif %}
    <!-- END google analytics -->
    <script>var token='{{ response.meta.antiCsrfToken }}';</script>
</head>
<body>
<header>
    {% block header %}
        {% block navbar %}
        <nav class="navbar" role="banner">
            <div class="navbar-header">
                <div class="container relposn">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <div class="brand">
                        <a href="/">
                            <span>/</span>
                        </a>
                    </div>
                </div><!--/.container-->
            </div>
            <div class="collapse navbar-collapse">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <ul class="nav navbar-nav">
                                <li class="">
                                    <a href="//{{ domain }}/"><i class="fa fa-home" aria-hidden="true"></i></a>
                                </li>
                                {% for nav in template.navigation %}
                                    {% if nav.dotClass = 'SWD.Structures.Navigation.Single' %}
                                        <li><a href="//{{ domain }}{{ nav.link }}">{{ nav.text }}</a></li>
                                    {% elseif nav.dotClass = 'SWD.Structures.Navigation.Collection' %}
                                        <li>
                                            <a href="{{ nav.link }}" data-toggle="dropdown" class="dropdown-toggle">{{ nav.text }}<i
                                                        class="fa fa-angle-down" aria-hidden="true"></i></a>
                                            <ul class="dropdown-menu">
                                                {% for subnav in nav %}
                                                    <li><a href="{{ subnav.link }}">{{ subnav.text }}</a></li>
                                                {% endfor %}
                                            </ul>
                                        </li>
                                    {% endif %}
                                {% endfor %}
                            </ul>
                        </div>
                    </div>
                </div><!--/.container-->
            </div>
        </nav><!--/nav-->
        {% endblock %}
    {% endblock %}
</header>
{% block container %}
    <div class="container">
        {% block h1 %}
            <h1>{{ seo.h1 }}</h1>
        {% endblock %}
        {% block content %}
            <div class="content">
                {{ response.data }}
            </div>
        {% endblock %}
    </div>
{% endblock %}
{% block modal %}
    <div class="modal fade" id="modal" tabindex="-1" role="dialog" aria-labelledby="Modal">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">Ã—</span></button>
                </div>
                <div class="modal-body">
                </div>
                <div class="modal-footer">
                    <button type="button" class="js-modal2-dismiss btn btn-default" data-dismiss="modal">Continue</button>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
{% block footer %}
    <footer>
        <div class="container">
            <div class="row">
                <div class="col-sm-6 col-xs-12">
                    <div class="row">
                        <div class="col-xs-6"></div>
                        <div class="col-xs-6"></div>
                    </div>
                </div>
                <div class="col-sm-6 col-xs-12">
                    <div class="row">
                        <div class="col-xs-6"></div>
                        <div class="col-xs-6"></div>
                    </div>
                </div>
            </div>
        </div>
        <div class="container-fluid">
            <div class="row footer-bottom">
                <div class="container">
                    <div class="row">
                        <div class="col-xs-12">
                            <div class="row">
                                <div class="col-sm-6">
                                    Copyright &copy; {{"now"|date('Y') }} {{ domain }}, All Rights Reserved.
                                </div>
                                <div class="col-sm-6">
                                    Powered by <a href="https://shermanwebdesign.com">Sherman Web Design</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
{% endblock %}
<script src="https://code.jquery.com/jquery-1.11.3.min.js"
        integrity="sha256-7LkWEzqTdpEfELxcZZlS6wAx5Ff13zZ83lYO2/ujj7g="
        crossorigin="anonymous"
></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/js-cookie@2/src/js.cookie.min.js"></script>
<script src="//{{ domain }}{{ js('/js/custom.js') }}"></script>
{% if response.meta.antiCsrfToken %}
    <script src="/api/public.js"></script>
    <script>var publicApi=new PublicApi('{{ response.meta.antiCsrfToken }}')</script>
{% endif %}

<script>
    $(document).ready(function(){
        var m = Cookies.get('message')||null;
        if(m){
            var modal = $('#modal');
            modal.modal();
            modal.find('.modal-body').text(atob(m));
            Cookies.remove('message');
        }
    })
</script>
<script>
    (function () {
        var a = document.getElementsByClassName('post-load');
        for (i = 0, c = a.length; i < c; i++) {a[i].setAttribute("href", a[i].getAttribute("data-href"));}
    })();
</script>
<!--https://github.com/medialize/URI.js-->
<script src="//{{ domain }}{{ js('/js/uri.js') }}"></script>
{% block javascript %}{% endblock %}
</body>
</html>